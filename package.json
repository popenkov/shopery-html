{
  "name": "project-builder",
  "version": "2.0.0-beta",
  "description": "Start kit for HTML(PUG)/CSS(SCSS)/JS pages layout",
  "author": "Alexander Motorygin (https://github.com/motosasha)",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motosasha/project-builder-v2"
  },
  "type": "module",
  "node version": "18+",
  "scripts": {
    "test": "npm run test:pug && npm run test:style && npm run test:js",
    "test:pug": "glob-exec 'src/**/*.pug' -- 'pug-lint {{files.join(\" \")}}'",
    "test:style": "stylelint 'src/**/*.scss'",
    "test:js": "eslint src/**/*.js",
    "test:ec": "editorconfig-cli '*.{js,pug,md}'",
    "fix:style": "stylelint 'src/**/*.scss' --fix",
    "fix:js": "eslint src/**/*.js --fix",
    "prettier:js": "prettier --check 'src/**/*.js'",
    "prettier-fix:js": "prettier --write 'src/**/*.js'",
    "start": "gulp",
    "library": "cross-env BUILD_LIBRARY=true gulp",
    "build": "cross-env MODE=production gulp build",
    "clear": "gulp dev:clear",
    "lint-staged": "lint-staged",
    "deploy": "cross-env MODE=deploy gulp deploy",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,pug,md}": "editorconfig-cli",
    "*.pug": "pug-lint",
    "*.scss": "stylelint --custom-syntax postcss-scss",
    "*.js": "eslint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie > 0",
    "not ie_mob > 0",
    "not dead"
  ],
  "additionalsDependencies": {
    "linux": {
      "description": "if linux add rollup-linux-x64-gnu",
      "package": "'@rollup/rollup-linux-x64-gnu': '^4.17.2'"
    },
    "windows": {
      "description": "if windows add rollup-linux-x64-gnu",
      "package": "'@rollup/rollup-win32-x64-msvc': '^4.17.2'"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/eslint-parser": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@eslint/js": "^9.3.0",
    "@htmlacademy/editorconfig-cli": "^3.0.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "browser-sync": "^3.0.2",
    "chalk": "^5.3.0",
    "cpy": "^11.0.1",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "gh-pages": "^6.1.1",
    "glob-exec": "^0.1.1",
    "globals": "^15.3.0",
    "gulp": "^5.0.0",
    "gulp-cheerio": "^1.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^5.0.1",
    "gulp-json-concat": "^0.2.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-pretty-html": "^2.0.10",
    "gulp-pug": "^5.0.0",
    "gulp-pug-linter": "^1.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svgo": "^2.2.1",
    "gulp-svgstore": "^9.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "mkdirp": "^3.0.1",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-pxtorem": "^6.1.0",
    "postcss-replace": "^2.0.1",
    "postcss-scss": "^4.0.9",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.2.5",
    "pug-lint": "^2.7.0",
    "puglint-stylish": "^1.1.0",
    "rollup": "^4.18.0",
    "sass": "^1.77.3",
    "stylelint": "^16.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.0",
    "stylelint-selector-bem-pattern": "^4.0.0",
    "through2": "^4.0.2"
  },
  "dependencies": {
    "hystmodal": "^1.0.1",
    "imask": "^7.6.0",
    "js-cookie": "^3.0.5",
    "just-validate": "^4.3.0",
    "nice-select2": "^2.1.0",
    "range-slider-input": "^2.4.4",
    "sanitize.css": "^13.0.0",
    "swiper": "^11.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "volta": {
    "node": "20.11.0"
  }
}
